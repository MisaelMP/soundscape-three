{
	"version": 2,
	"buildCommand": "npm run build",
	"outputDirectory": "dist",
	"framework": "vite",
	"rewrites": [
		{
			"source": "/api/(.*)",
			"destination": "/api/$1"
		},
		{
			"source": "/socket.io/(.*)",
			"destination": "/api/socket.io/$1"
		},
		{
			"source": "/(.*)",
			"destination": "/"
		}
	],
	"env": {
		"VITE_BACKEND_URL": "@vite_backend_url"
	}
}
